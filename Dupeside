DungeonBox:AddButton({
    Text = "Potential Teleport",
    Func = function()
        local Player = Players.LocalPlayer
        local PlayerHRP = (Player.Character or Player.CharacterAdded:Wait()):WaitForChild("HumanoidRootPart")
        local Potentials = workspace:FindFirstChild("Potentials")

        if Potentials then
            local NearestDistance = math.huge
            local NearestPotential = nil

            for _, Potential in pairs(Potentials:GetChildren()) do
                if Potential:IsA("BasePart") and Potential.Name:lower():match("^room") then
                    local Distance = (PlayerHRP.Position - Potential.Position).Magnitude

                    if Distance < NearestDistance then
                        NearestPotential = Potential
                        NearestDistance = Distance
                    end
                end
            end

            if NearestPotential then
                PlayerHRP.CFrame = NearestPotential.CFrame + Vector3.new(0, 5, 0)
            end
        end
    end,
    DoubleClick = false,
    Tooltip = "Teleport to potential"
})

DungeonBox:AddButton({
    Text = "Boss Room Teleport",
    Func = function()
        local Player = Players.LocalPlayer
        local PlayerHRP = (Player.Character or Player.CharacterAdded:Wait()):WaitForChild("HumanoidRootPart")
        local BossDoor = workspace:FindFirstChild("MAP") and workspace.MAP:FindFirstChild("Room999")

        if BossDoor then
            BossDoor = workspace.MAP.Room999:FindFirstChild("BossDoor")
        end

        if BossDoor and BossDoor:IsA("BasePart") then
            PlayerHRP.CFrame = BossDoor.CFrame + Vector3.new(0, 5, 0)
        end
    end,
    DoubleClick = false,
    Tooltip = "Teleport To Boss Door"
})

DungeonBox:AddButton({
    Text = "Item Teleport",
    Func = function()
        local Player = Players.LocalPlayer
        local PlayerHRP = (Player.Character or Player.CharacterAdded:Wait()):WaitForChild("HumanoidRootPart")
        local OtherChars = workspace:FindFirstChild("OtherChars")

        if OtherChars then
            local NearestDistance = math.huge
            local NearestItem = nil

            for _, Item in pairs(OtherChars:GetChildren()) do
                if Item:IsA("Model") then
                    local ItemPosition = nil

                    if Item.PrimaryPart then
                        ItemPosition = Item.PrimaryPart.Position
                    else
                        local Descendants = Item:GetDescendants()
                        local TotalPosition = Vector3.new(0, 0, 0)
                        local Count = 0

                        for _, Descendant in pairs(Descendants) do
                            if Descendant:IsA("BasePart") then
                                TotalPosition = TotalPosition + Descendant.Position
                                Count = Count + 1
                            end
                        end

                        if Count > 0 then
                            ItemPosition = TotalPosition / Count
                        end
                    end

                    if ItemPosition then
                        local Distance = (PlayerHRP.Position - ItemPosition).Magnitude

                        if Distance < NearestDistance then
                            NearestItem = Item
                            NearestDistance = Distance
                        end
                    end
                end
            end

            if NearestItem then
                local TargetCFrame = nil

                if NearestItem.PrimaryPart then
                    TargetCFrame = NearestItem.PrimaryPart.CFrame
                else
                    local FirstPart = NearestItem:FindFirstChildWhichIsA("BasePart")

                    if FirstPart then
                        TargetCFrame = FirstPart.CFrame
                    end
                end

                if TargetCFrame then
                    PlayerHRP.CFrame = TargetCFrame + Vector3.new(0, 5, 0)
                end
            end
        else
            warn("[DupeSide] workspace.OtherChars not found.")
        end
    end,
    DoubleClick = false,
    Tooltip = "Teleport To Closest item/otherchar"
})

