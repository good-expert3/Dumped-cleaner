QOLBox:AddToggle("AutoSprint", {
    Text = "Auto Sprint",
    Default = getgenv().AutoSprint,
    Callback = function(Value)
        if AutoSprintInitialized then
            getgenv().AutoSprint = Value
            local Player = Players.LocalPlayer

            if Player then
                local RequestAction = Player:WaitForChild("Remotes"):WaitForChild("RequestAction")

                if Value then
                    pcall(function()
                        RequestAction:InvokeServer("Sprint", Enum.UserInputState.Begin, nil, "Start")
                    end)
                else
                    pcall(function()
                        RequestAction:InvokeServer("Sprint", Enum.UserInputState.End, nil, "End")
                    end)
                end
            end
        else
            AutoSprintInitialized = true
        end
    end
})

