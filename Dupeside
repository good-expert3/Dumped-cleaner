task.wait(1)

task.spawn(function()
    while true do
        repeat
            task.wait(0.1)
        until Toggles.AutoAttack.Value

        local Remotes = LocalPlayer:FindFirstChild("Remotes")

        if Remotes and Remotes:FindFirstChild("RequestAction") then
            pcall(function()
                Remotes.RequestAction:InvokeServer("Weapon", Enum.UserInputState.Begin)
            end)
        end
    end
end)

local BodyVelocity = nil
local BodyGyro = nil
local IsFlying = false
local FlyRotation = CFrame.new()
local Camera = workspace.CurrentCamera

local KeysPressed = {
    W = false,
    A = false,
    S = false,
    D = false,
    Up = false,
    Down = false
}

local function CreateFlyBodies()
    local BV = Instance.new("BodyVelocity")
    BV.MaxForce = Vector3.new(100000, 100000, 100000)
    BV.Velocity = Vector3.new(0, 0, 0)
    BV.P = 1250

    local BG = Instance.new("BodyGyro")
    BG.MaxTorque = Vector3.new(100000, 100000, 100000)
    BG.P = 10000

    return BV, BG
end

local function AttachFlyBodies(Part)
    if not (BodyVelocity and BodyGyro) then
        BodyVelocity, BodyGyro = CreateFlyBodies()
    end

    BodyVelocity.Parent = Part
    BodyGyro.Parent = Part
end

local function DetachFlyBodies()
    if BodyVelocity then
        BodyVelocity.Parent = nil
    end

    if BodyGyro then
        BodyGyro.Parent = nil
    end
end

local function GetCharacterParts()
    local Char = LocalPlayer.Character

    if Char then
        local HRP = Char:FindFirstChild("HumanoidRootPart")
        local Hum = Char:FindFirstChildOfClass("Humanoid")

        if HRP and Hum then
            return Char, HRP, Hum
        end
    end
end

local function StartFlying()
    local _, HRP, Humanoid = GetCharacterParts()

    if HRP and Humanoid then
        IsFlying = true
        FlyRotation = HRP.CFrame - HRP.CFrame.p
        AttachFlyBodies(HRP)
        Humanoid.PlatformStand = false

        pcall(function()
            Humanoid:ChangeState(Enum.HumanoidStateType.Freefall)
        end)
    end
end

local function StopFlying()
    IsFlying = false
    DetachFlyBodies()

    local _, _, Humanoid = GetCharacterParts()

    if Humanoid then
        pcall(function()
            Humanoid:ChangeState(Enum.HumanoidStateType.Landed)
        end)
    end
end

local NoclipConnection = nil

local function EnableNoclip()
    if not NoclipConnection then
        NoclipConnection = RunService.Stepped:Connect(function()
            if game.Players.LocalPlayer.Character then
                for _, Part in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                    if Part:IsA("BasePart") and Part.CanCollide then
                        Part.CanCollide = false
                    end
                end
            end
        end)
    end
end

local function DisableNoclip()
    if NoclipConnection then
        NoclipConnection:Disconnect()
        NoclipConnection = nil
    end
end

UserInputService.InputBegan:Connect(function(Input, GameProcessed)
    if not GameProcessed then
        if Input.UserInputType == Enum.UserInputType.Keyboard then
            local KeyCode = Input.KeyCode

            if KeyCode == Enum.KeyCode.W then
                KeysPressed.W = true
            elseif KeyCode == Enum.KeyCode.A then
                KeysPressed.A = true
            elseif KeyCode == Enum.KeyCode.S then
                KeysPressed.S = true
            elseif KeyCode == Enum.KeyCode.D then
                KeysPressed.D = true
            elseif KeyCode == Enum.KeyCode.Space then
                KeysPressed.Up = true
            elseif KeyCode == Enum.KeyCode.LeftControl or KeyCode == Enum.KeyCode.RightControl then
                KeysPressed.Down = true
            end
        end
    end
end)

UserInputService.InputEnded:Connect(function(Input)
    if Input.UserInputType == Enum.UserInputType.Keyboard then
        local KeyCode = Input.KeyCode

        if KeyCode == Enum.KeyCode.W then
            KeysPressed.W = false
        elseif KeyCode == Enum.KeyCode.A then
            KeysPressed.A = false
        elseif KeyCode == Enum.KeyCode.S then
            KeysPressed.S = false
        elseif KeyCode == Enum.KeyCode.D then
            KeysPressed.D = false
        elseif KeyCode == Enum.KeyCode.Space then
            KeysPressed.Up = false
        elseif KeyCode == Enum.KeyCode.LeftControl or KeyCode == Enum.KeyCode.RightControl then
            KeysPressed.Down = false
        end
    end
end)

RunService.RenderStepped:Connect(function(DeltaTime)
    Camera = workspace.CurrentCamera or Camera

    if Toggles.FlyToggle and Toggles.FlyToggle.Value then
        if not IsFlying then
            StartFlying()
        end

        local _, HRP = GetCharacterParts()

        if HRP and BodyVelocity then
            local Speed = Options.FlySpeed and Options.FlySpeed.Value or (getgenv().FlySpeed or 20)
            local ForwardBack = (KeysPressed.W and 1 or 0) - (KeysPressed.S and 1 or 0)
            local LeftRight = (KeysPressed.D and 1 or 0) - (KeysPressed.A and 1 or 0)
            local UpDown = (KeysPressed.Up and 1 or 0) - (KeysPressed.Down and 1 or 0)

            local CamCFrame = Camera and Camera.CFrame or HRP.CFrame
            local LookVector = CamCFrame.LookVector
            local RightVector = CamCFrame.RightVector

            local Direction = LookVector * ForwardBack + RightVector * LeftRight + Vector3.new(0, 1, 0) * UpDown

            if Direction.Magnitude > 0 then
                Direction = Direction.Unit
            end

            BodyVelocity.Velocity = Direction * Speed

            if BodyGyro then
                BodyGyro.CFrame = CFrame.new(HRP.Position) * (FlyRotation or CFrame.new())
            end
        end
    elseif IsFlying then
        StopFlying()
    end

    if Toggles.SpeedActive and Toggles.SpeedActive.Value then
        local Speed = Options.SpeedValue and Options.SpeedValue.Value or (getgenv().SpeedValue or 50)
        local Char = LocalPlayer.Character

        if Char and Char:FindFirstChild("HumanoidRootPart") then
            local Humanoid = Char:FindFirstChildOfClass("Humanoid")

            if Humanoid and Humanoid.MoveDirection.Magnitude > 0 then
                local HRP = Char.HumanoidRootPart
                HRP.CFrame = HRP.CFrame + Humanoid.MoveDirection * (Speed * DeltaTime)
            end
        end
    end

    if Toggles.NoclipToggle and Toggles.NoclipToggle.Value then
        EnableNoclip()
    else
        DisableNoclip()
    end
end)

UserInputService.InputBegan:Connect(function(Input, GameProcessed)
    if GameProcessed then
        return
    end

    if UserInputService and UserInputService:GetFocusedTextBox() then
        return
    end

    if Input.UserInputType == Enum.UserInputType.Keyboard then
        local Key = tostring(Input.KeyCode):gsub("Enum.KeyCode.", "")

        if Key == FlyKey then
            if Toggles.FlyToggle then
                Toggles.FlyToggle:SetValue(not Toggles.FlyToggle.Value)
            end
        elseif Key == SpeedKey then
            if Toggles.SpeedActive then
                Toggles.SpeedActive:SetValue(not Toggles.SpeedActive.Value)
            end
        elseif Key == NoclipKey then
            if Toggles.NoclipToggle then
                Toggles.NoclipToggle:SetValue(not Toggles.NoclipToggle.Value)
            end
        end
    end
end)

local function CreateBossRoomESP(Part)
    if Part and not Part:FindFirstChild("SixSeven") then
        local Billboard = Instance.new("BillboardGui")
        Billboard.Name = "SixSeven"
        Billboard.Size = UDim2.new(0, 120, 0, 40)
        Billboard.AlwaysOnTop = true
        Billboard.StudsOffset = Vector3.new(0, 4, 0)
        Billboard.Adornee = Part

        local Label = Instance.new("TextLabel")
        Label.BackgroundTransparency = 1
        Label.Size = UDim2.new(1, 0, 1, 0)
        Label.Text = "Boss Room"
        Label.TextColor3 = Color3.fromRGB(255, 140, 0)
        Label.TextStrokeTransparency = 0.2
        Label.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
        Label.TextScaled = true
        Label.Font = Enum.Font.GothamBold
        Label.Parent = Billboard

        Billboard.Parent = Part
    end
end

task.spawn(function()
    while true do
        repeat
            task.wait(2)
        until Toggles.BossRoomESP and Toggles.BossRoomESP.Value

        for _, Descendant in ipairs(workspace:GetDescendants()) do
            if Descendant.Name == "BossDoor" and Descendant:IsA("BasePart") then
                CreateBossRoomESP(Descendant)
            end
        end
    end
end)

local ESPLibrarySuccess, ESPLibrary = pcall(function()
    return loadstring(game:HttpGet("https://raw.githubusercontent.com/linemaster2/esp-library/main/library.lua"))()
end)

local NPCESPSuccess, NPCESPLibrary = pcall(function()
    return loadstring(game:HttpGet("https://raw.githubusercontent.com/iusedoneyed-hub/example/refs/heads/main/npc_esp.lua"))()
end)

local _, ItemESPLibrary = pcall(function()
    return loadstring(game:HttpGet("https://raw.githubusercontent.com/DerekLuaU/example/refs/heads/main/esp_items.lua"))()
end)

local PlayerESP = not ESPLibrarySuccess and {
    Enabled = false,
    ShowBox = false,
    ShowName = false,
    ShowHealth = false,
    ShowTracer = false,
    ShowDistance = false
} or ESPLibrary

local NPCESP = not NPCESPSuccess and {
    Enabled = false,
    ShowBox = false,
    ShowName = false,
    ShowHealthNumber = false,
    ShowTracer = false,
    ShowDistance = false
} or NPCESPLibrary

task.spawn(function()
    while true do
        task.wait(0.6)

        local ESPEnabled = Options.ESPEnabled and Options.ESPEnabled.Value or getgenv().ESPSettings.Enabled
        local ESPBox = Options.ESPBox and Options.ESPBox.Value or getgenv().ESPSettings.ShowBox
        local ESPName = Options.ESPName and Options.ESPName.Value or getgenv().ESPSettings.ShowName

        if PlayerESP then
            PlayerESP.Enabled = ESPEnabled
            PlayerESP.ShowBox = ESPBox
            PlayerESP.ShowName = ESPName
            PlayerESP.ShowHealth = Options.ESPHealth and Options.ESPHealth.Value or getgenv().ESPSettings.ShowHealth
            PlayerESP.ShowTracer = Options.ESPTracer and Options.ESPTracer.Value or getgenv().ESPSettings.ShowTracer
            PlayerESP.ShowDistance = Options.ESPDistance and Options.ESPDistance.Value or getgenv().ESPSettings.ShowDistance
        end

        if NPCESP then
            NPCESP.Enabled = getgenv().NPCESPSettings.Enabled
            NPCESP.ShowBox = getgenv().NPCESPSettings.ShowBox
            NPCESP.ShowName = getgenv().NPCESPSettings.ShowName
            NPCESP.ShowHealthNumber = getgenv().NPCESPSettings.ShowHealthNumber
            NPCESP.ShowTracer = getgenv().NPCESPSettings.ShowTracer
            NPCESP.ShowDistance = getgenv().NPCESPSettings.ShowDistance
        end

        if ItemESPLibrary then
            if type(ItemESPLibrary.SetEnabled) == "function" then
                pcall(function()
                    ItemESPLibrary.SetEnabled(getgenv().ItemESPSettings.Enabled)
                end)
            end

            if type(ItemESPLibrary.SetBoxEnabled) == "function" then
                pcall(function()
                    ItemESPLibrary.SetBoxEnabled(getgenv().ItemESPSettings.ShowBox)
                end)
            end

            if type(ItemESPLibrary.SetNameEnabled) == "function" then
                pcall(function()
                    ItemESPLibrary.SetNameEnabled(getgenv().ItemESPSettings.ShowName)
                end)
            end

            if type(ItemESPLibrary.SetDistanceEnabled) == "function" then
                pcall(function()
                    ItemESPLibrary.SetDistanceEnabled(getgenv().ItemESPSettings.ShowDistance)
                end)
            end
        end
    end
end)

local function UseSkill(SkillName)
    if SkillName ~= "" then
        local Args = { SkillName, Enum.UserInputState.Begin }
        Players.LocalPlayer:WaitForChild("Remotes"):WaitForChild("RequestAction"):InvokeServer(unpack(Args))
        print("[DupeSide] Used skill:", SkillName)
    end
end

local function IsNPCNearby(Range)
    local Player = game.Players.LocalPlayer

    if not (Player.Character and Player.Character:FindFirstChild("HumanoidRootPart")) then
        return false
    end

    local PlayerHRP = Player.Character.HumanoidRootPart

    for _, NPC in pairs(workspace.NPCs:GetChildren()) do
        if NPC:IsA("Model") and NPC:FindFirstChild("HumanoidRootPart") then
            if (NPC.HumanoidRootPart.Position - PlayerHRP.Position).Magnitude <= Range then
                return true
            end
        end
    end

    return false
end

spawn(function()
    while true do
        while AutoSkillToggle.Value do
            if IsNPCNearby(20) then
                UseSkill(AttackSkill1)
                task.wait(1)
                UseSkill(AttackSkill2)
                task.wait(1)
            else
                task.wait(0.5)
            end
        end

        task.wait(0.5)
    end
end)

local LastParryTime = 0

local AttackKeywords = {
    "attack", "swing", "crit", "skill", "slash", "pierce", "impact",
    "arrow release", "hit", "kick", "stoneslashes", "projectile",
    "fireball", "iceblast", "shockwave", "charge", "meteor", "aoe",
    "fist", "FrozenCore", "Beam", "FrozenCoreBeam", "Explosion"
}

local function MatchAttackKeyword(Name)
    local LowerName = Name:lower()

    for _, Keyword in pairs(AttackKeywords) do
        if string.find(LowerName, Keyword) then
            return Keyword:upper()
        end
    end

    return nil
end

local function ExecuteParry()
    local ParryArgs = { "Parry", Enum.UserInputState.Begin }

    if LocalPlayer and LocalPlayer:FindFirstChild("Remotes") and LocalPlayer.Remotes:FindFirstChild("RequestAction") then
        pcall(function()
            LocalPlayer.Remotes.RequestAction:InvokeServer(unpack(ParryArgs))
        end)
    end
end

local function HandleAutoParry(Source, AttackType, NPC)
    if Toggles.AutoParryEnabled and Toggles.AutoParryEnabled.Value then
        local PlayerHRP = (LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()):WaitForChild("HumanoidRootPart")
        local SourcePosition

        if Source:IsA("BasePart") then
            SourcePosition = Source.Position
        elseif Source:IsA("Sound") and Source.Parent and Source.Parent:IsA("BasePart") then
            SourcePosition = Source.Parent.Position
        elseif Source:IsA("AnimationTrack") and NPC:FindFirstChild("HumanoidRootPart") then
            SourcePosition = NPC.HumanoidRootPart.Position
        else
            return
        end

        local ParryRange = Options.ParryRange and Options.ParryRange.Value or (getgenv().ParryRange or 30)
        local ParryCooldown = Options.ParryCooldown and Options.ParryCooldown.Value or (getgenv().ParryCooldown or 0.5)
        local ParryCritDelay = Options.ParryCritDelay and Options.ParryCritDelay.Value or (getgenv().ParryCritDelay or 0.2)

        if (SourcePosition - PlayerHRP.Position).Magnitude <= ParryRange then
            local CurrentTime = tick()

            if CurrentTime - LastParryTime >= ParryCooldown then
                LastParryTime = CurrentTime
                local Delay = AttackType == "CRIT" and ParryCritDelay or 0
                task.delay(Delay, ExecuteParry)
            end
        end
    end
end

local function SetupNPCParryDetection(NPC)
    local NPCHRP = NPC:FindFirstChild("HumanoidRootPart")

    if NPCHRP then
        for _, Child in pairs(NPCHRP:GetChildren()) do
            local AttackType = MatchAttackKeyword(Child.Name)

            if AttackType then
                HandleAutoParry(Child, AttackType, NPC)
            end
        end

        NPCHRP.ChildAdded:Connect(function(NewChild)
            local AttackType = MatchAttackKeyword(NewChild.Name)

            if AttackType then
                HandleAutoParry(NewChild, AttackType, NPC)
            end
        end)

        local Humanoid = NPC:FindFirstChildOfClass("Humanoid")
        local Animator = Humanoid and Humanoid:FindFirstChildOfClass("Animator")

        if Animator then
            Animator.AnimationPlayed:Connect(function(AnimationTrack)
                local AttackType = MatchAttackKeyword(AnimationTrack.Name)

                if AttackType then
                    HandleAutoParry(AnimationTrack, AttackType, NPC)
                end
            end)
        end
    end
end

for _, NPC in pairs(NPCsFolder:GetChildren()) do
    if NPC:IsA("Model") then
        SetupNPCParryDetection(NPC)
    end
end

NPCsFolder.ChildAdded:Connect(function(NewNPC)
    if NewNPC:IsA("Model") then
        task.wait(0.2)
        SetupNPCParryDetection(NewNPC)
    end
end)

Library:OnUnload(function()
    DisableNoclip()

    if BodyVelocity then
        BodyVelocity:Destroy()
    end

    if BodyGyro then
        BodyGyro:Destroy()
    end

    print("DupeSide merged UI unloaded.")
end)

Library:Notify({
    Title = "DupeSide",
    Description = "Succesfully loaded.",
    Time = 4
})

task.delay(1, function()
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "DupeSide Hub",
        Text = "discord.gg/mcPTS824Fc",
        Duration = 6
    })
end)
