}):AddKeyPicker("FlyKeyPicker", {
    Default = "K",
    SyncToggleState = true,
    Mode = "Toggle",
    Text = "Fly Key",
    NoUI = false,
    Callback = function(Value)
        getgenv().FlyEnabled = Value

        if Toggles and Toggles.FlyToggle then
            Toggles.FlyToggle:SetValue(Value)
        end
    end,
    ChangedCallback = function(NewKey)
        if NewKey then
            FlyKey = tostring(NewKey):gsub("Enum.KeyCode.", "")
        end

        print("[cb] Fly Keybind changed! ->", FlyKey)
    end
})

