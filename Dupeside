local AutoSprintInitialized = false
local AutoPotentialActive = false
local AttackSkill1 = ""
local AttackSkill2 = ""
local TeleportMethod = "Above"
local CurrentTarget = nil
local LastTeleportTime = 0
local TeleportInterval = 0.016666666666666666

local NPCsFolder = Workspace:FindFirstChild("NPCs")
if not NPCsFolder then
    NPCsFolder = Instance.new("Folder")
    NPCsFolder.Name = "NPCs"
    NPCsFolder.Parent = Workspace
end

local BossKeywords = {
    "larimar",
    "rogue",
    "stonewatcher",
    "humanoidboss",
    "boss"
}

local function IsBoss(Name)
    local LowerName = Name:lower()
    for _, Keyword in ipairs(BossKeywords) do
        if LowerName:find(Keyword) then
            return true
        end
    end
    return false
end

local function FindNearestNPC(Position)
    local NearestBossDistance = math.huge
    local NearestNPCDistance = math.huge
    local NearestBoss = nil
    local NearestNPC = nil

    for _, NPC in ipairs(NPCsFolder:GetChildren()) do
        if NPC:IsA("Model") then
            local NPCHRP = NPC:FindFirstChild("HumanoidRootPart")
            local NPCHumanoid = NPC:FindFirstChildOfClass("Humanoid")

            if NPCHRP and NPCHumanoid and NPCHumanoid.Health > 0 then
                local Distance = (NPCHRP.Position - Position).Magnitude

                if IsBoss(NPC.Name:lower()) then
                    if Distance < NearestBossDistance then
                        NearestBoss = NPC
                        NearestBossDistance = Distance
                    end
                elseif Distance < NearestNPCDistance then
                    NearestNPC = NPC
                    NearestNPCDistance = Distance
                end
            end
        end
    end

    return NearestBoss or NearestNPC
end

local function TeleportTo(TargetCFrame)
    if HumanoidRootPart and Character then
        HumanoidRootPart.Velocity = Vector3.zero
        HumanoidRootPart.RotVelocity = Vector3.zero

        pcall(function()
            Character:SetPrimaryPartCFrame(TargetCFrame)
            local Humanoid = Character:FindFirstChildOfClass("Humanoid")
            if Humanoid then
                Humanoid.AutoRotate = false
            end
        end)
    end
end

local function UpdateAutoFarm()
    if Toggles.TeleportToggle.Value then
        if HumanoidRootPart then
            if CurrentTarget and CurrentTarget.Parent then
                local CurrentTime = time()

                if CurrentTime - LastTeleportTime >= TeleportInterval then
                    LastTeleportTime = CurrentTime
                    local TargetHRP = CurrentTarget:FindFirstChild("HumanoidRootPart")

                    if TargetHRP then
                        local TargetCFrame = TargetHRP.CFrame
                        local Distance = IsBoss(CurrentTarget.Name:lower()) and Options.TeleportBossDistance.Value or Options.DistanceBehind.Value
                        local FinalCFrame

                        if TeleportMethod == "Behind" then
                            local Position = TargetCFrame.Position - TargetCFrame.LookVector * Distance
                            FinalCFrame = CFrame.lookAt(Position, TargetCFrame.Position, Vector3.new(0, 1, 0))
                        elseif TeleportMethod == "Above" then
                            local Position = TargetCFrame.Position + Vector3.new(0, Distance, 0)
                            FinalCFrame = CFrame.lookAt(Position, TargetCFrame.Position, Vector3.new(0, 1, 0))
                        elseif TeleportMethod == "Below" then
                            local Position = TargetCFrame.Position - Vector3.new(0, Distance, 0)
                            FinalCFrame = CFrame.lookAt(Position, TargetCFrame.Position, Vector3.new(0, 1, 0))
                        else
                            local Position = TargetCFrame.Position - TargetCFrame.LookVector * Distance
                            FinalCFrame = CFrame.lookAt(Position, TargetCFrame.Position, Vector3.new(0, 1, 0))
                        end

                        TeleportTo(FinalCFrame)
                    end
                end
            else
                CurrentTarget = FindNearestNPC(HumanoidRootPart.Position)
            end
        end
    end
end

RunService.RenderStepped:Connect(UpdateAutoFarm)

