    Default = getgenv().BossRoomESP or false,
    Callback = function(Value)
        getgenv().BossRoomESP = Value

        if not Value then
            for _, Descendant in ipairs(workspace:GetDescendants()) do
                if Descendant.Name == "BossDoor" and Descendant:IsA("BasePart") then
                    local ESP = Descendant:FindFirstChild("SixSeven")

                    if ESP then
                        ESP:Destroy()
                    end
                end
            end
        end
    end
