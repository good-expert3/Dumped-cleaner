AutoFarmBox:AddToggle("AutoStartDungeon", {
    Text = "Auto Start Dungeon",
    Default = false,
    Callback = function(Value)
        getgenv().AutoStartDungeon = Value

        if Value then
            task.spawn(function()
                local AffixUI = Players.LocalPlayer.PlayerGui:WaitForChild("AffixNumSelection")
                local ReadyUpRemote = game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("ReadyUp")
                local HasStarted = false

                while getgenv().AutoStartDungeon do
                    if AffixUI.Enabled ~= true then
                        HasStarted = false
                    elseif not HasStarted then
                        HasStarted = true
                        task.wait(3)
                        ReadyUpRemote:FireServer({})
                        task.wait(1)

                        if Toggles.TeleportToggle then
                            Toggles.TeleportToggle:SetValue(true)
                        end

                        Library:Notify({
                            Title = "DupeSide",
                            Description = "Dungeon Started!",
                            Time = 4
                        })
                    end

                    task.wait(0.2)
                end
            end)
        end
    end
})

AutoFarmBox:AddToggle("AutoContinueDungeon", {
    Text = "Auto Continue Dungeon",
    Default = false,
    Callback = function(Value)
        getgenv().AutoContinueDungeon = Value

        if Value then
            task.spawn(function()
                local Player = Players.LocalPlayer
                local PlayerGui = Player:WaitForChild("PlayerGui")
                local PreviousState = false

                while getgenv().AutoContinueDungeon do
                    local LayerComplete = nil

                    if Toggles.TeleportToggle and Toggles.TeleportToggle.Value == true then
                        LayerComplete = PlayerGui:FindFirstChild("LayerComplete")

                        if LayerComplete then
                            LayerComplete = LayerComplete.Enabled
                        end

                        if LayerComplete and not PreviousState then
                            if Toggles.TeleportToggle then
                                Toggles.TeleportToggle:SetValue(false)
                            end

                            task.wait(3)

                            if pcall(function()
                                return Player:WaitForChild("Remotes"):WaitForChild("DungeonCompleted"):InvokeServer("Continue")
                            end) then
                                Library:Notify({
                                    Title = "DupeSide",
                                    Description = "Continuing dungeon.",
                                    Time = 4
                                })
                            end
                        end
                    else
                        LayerComplete = PreviousState
                    end

                    task.wait(0.2)
                    PreviousState = LayerComplete
                end
            end)
        end
    end
})

